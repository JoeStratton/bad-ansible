[frontends] # These are the frontend servers
frontend1 ansible_host=frontend1.27b1.internal

[apps] ## These are the apps servers
app1 ansible_host=app1.27b1.internal
app2 ansible_host=app2.27b1.internal

[appdbs] # These are the appdb servers
appdb1 ansible_host=appdb1.27b1.internal

[internal:children]
frontends
apps
appdbs
